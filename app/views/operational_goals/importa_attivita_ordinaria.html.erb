<p id="notice"><%= notice %></p>

<h1>Obiettivi importati</h1>

<table>
  <thead>
    <tr>
      <th>Denominazione</th>
	  <th>Descrizione</th>
      <th>Responsabile</th>
      <th>Anno</th>
      
    </tr>
  </thead>

  <tbody>
    <% if @obiettivi != nil %>
    <% @obiettivi.each do |obiettivo| %>
      <tr>
        <td><%= obiettivo.denominazione %></td>
		<td><%= obiettivo.descrizione %></td>
        <td><%= obiettivo.responsabile_principale != nil ? obiettivo.responsabile_principale.nominativo : " - "  %></td>
        <td><%= obiettivo.anno %></td>
        </tr>
    <% end %>
	<% end %>
  </tbody>
</table>

<br>
<%= if @obiettivi.length == 0 then "Non sono stati importati obiettivi" else "NÂ° obiettivi importati :" + @obiettivi.length.to_s end %>
<br>

<hr>


