<p id="notice"><%= notice %></p>

<h1>Qualifiche</h1>

<table>
  <thead>
    <tr>
      <th>Denominazione</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @qualification_types.each do |qualification_type| %>
      <tr>
        <td><%= qualification_type.denominazione %></td>
        <td><%= link_to 'Show', qualification_type %></td>
        <td><%= link_to 'Edit', edit_qualification_type_path(qualification_type) %></td>
        <td><%= link_to 'Destroy', qualification_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Qualification Type', new_qualification_type_path %>
