

var sid='<%= "#s" + @p.id.to_s %>';
var tid='<%= "#t" + @p.id.to_s %>';
console.log(sid);
var element1 = $(sid);
var element2 = $(tid);
element1.empty();
element2.empty();
var html1 = '';
var html2 = '';


html1 += '	<%= form_for :person, url: set_servizio_tempo_people_path, method: :post, :remote => true do |f| %>';
html1 += '	<%= f.number_field :value_servizio_percentuale,  :value => @p.servizio_percentuale.to_f.round(2), min: 0, max: 1, step: :any, :onchange => "abilita_save_s("+@p.id.to_s+")" %>';
