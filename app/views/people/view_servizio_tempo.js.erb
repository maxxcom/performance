html1 += '   <% end %>';

element1.append(html1);

html2 += '<%= form_for :person, url: set_servizio_tempo_people_path, method: :post, :remote => true do |f| %>';
html2 += '<%= f.number_field :value_tempo,  :value => @p.tempo.to_f.round(2), min: 0, max: 1, step: :any, :onchange => "abilita_save_t("+@p.id.to_s+")" %>';
