  <thead>
    <tr>
      <th>Nome</th>
	  <th>Cognome</th>
      <th>Matricola</th>
      <th>email</th>
      <th>Categoria</th>
	  <th>Figura giuridica</th>
      <th>Ruolo</th>
	  <th>Note</th>
	  <th>Servizio %</th>
    </tr>
  </thead>

  <tbody>
    <% if @modificate != nil %>
    <% @modificate.each do |p| %>
      <tr>
        <td><%= p.nome %></td>
		<td><%= p.cognome %></td>
        <td><%= p.matricola %></td>
        <td><%= p.email %></td>
		<td><%= p.categoria %></td>
        <td><%= p.figura_giuridica %></td>
		<td><%= p.ruolo %></td>
		<td><%= p.note %></td>
		<td><%= p.servizio_percentuale %></td>
        </tr>
    <% end %>
	<% end %>
  </tbody>
</table>

