<p id="notice"><%= notice %></p>

<h1>Office Target Collaborations</h1>

<table>
  <thead>
    <tr>
      <th>Target</th>
      <th>Target type</th>
      <th>Office</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @office_target_collaborations.each do |office_target_collaboration| %>
      <tr>
        <td><%= office_target_collaboration.target_id %></td>
        <td><%= office_target_collaboration.target_type %></td>
        <td><%= office_target_collaboration.office_id %></td>
        <td><%= link_to 'Show', office_target_collaboration %></td>
        <td><%= link_to 'Edit', edit_office_target_collaboration_path(office_target_collaboration) %></td>
        <td><%= link_to 'Destroy', office_target_collaboration, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

